programa {
  funcao inicio() {
    inteiro num[2], result

    // entrada
    escreva("Qual primeiro número: ")
    leia(num[0])

    escreva("\nO segundo número: ")
    leia(num[1])
    
    // Processamento
    result = num[0] + num[1]

    // Saída
    escreva("A Soma entre ",num[0], " mais ",num[1]," é ingual a: ", result)

    // Fim
  }
}
